@model DiplomaOptions.Models.RegisterViewModel
@{
    ViewBag.Title = "Register";
}

<h2>@ViewBag.Title.</h2>

@using (Html.BeginForm())
{
    <fieldset>
        @Html.AntiForgeryToken()
        <legend>@ViewBag.Title</legend>
        @Html.EditorForModel()
        <p><input type="submit" value="Save" /></p>
    </fieldset>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
